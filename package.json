{
  "name": "rec-keep",
  "version": "0.0.1",
  "description": "Record keep application frontends",
  "scripts": {
    "build:js": "grunt build-js",
    "build:css_sass": "sass src/main/sass/app.scss src/main/resources/static/css/app.css",
